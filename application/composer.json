{
  "name": "aom/howhow",
  "description": "Document management system with more then loads of capabilities",
  "authors": [
    {
      "name": "Radu Dragomir",
      "email": "noramarth@aom.ro",
      "role": "Developer"
    }
  ],
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "HowHoW\\": "src/"
    }
  },
  "require": {
    "php": "^7.4",
    "react/event-loop":  "^1.1.0",
    "react/promise": "^2.7",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/console": "5.0.*",
    "symfony/dotenv": "5.0.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.0.*",
    "symfony/yaml": "5.0.*"
  },
  "require-dev": {
  },
  "autoload-dev": {
    "psr-4": {
      "AOM\\Tests\\HowHow\\": "tests/"
    }
  },

  "suggest": {
    "ext-apcu": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "scripts": {
    "auto-scripts": {
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  }
}
